{
  "name": "metrimask",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist",
    "start": "yarn clean && mkdir dist && ./scripts/create-empty-thunk.sh && webpack --config webpack.dev.config.js --watch --devtool eval-cheap-module-source-map & yarn css-types -w",
    "build": "yarn clean && webpack -p --config webpack.prod.config.js && ./scripts/create-empty-thunk.sh",
    "css-types": "tcm src",
    "postinstall": "yarn css-types",
    "linter": "tslint --project .",
    "linter-fix": "tslint --fix --project .",
    "precommit": "lint-staged",
    "prepush": "npm run linter-fix"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run linter-fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@ceoimon/typed-css-modules": "^0.0.1",
    "@material-ui/core": "^1.2.2",
    "@types/chrome": "^0.0.63",
    "@types/classnames": "^2.2.4",
    "@types/prop-types": "^15.5.3",
    "@types/react": "16.4.18",
    "@types/react-dom": "16.0.11",
    "@types/react-router-dom": "^4.2.7",
    "autoprefixer": "^9.8.8",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "node-sass": "^7.0.1",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.1.3",
    "postcss-modules": "^3.2.2",
    "react-router-dom": "^5.3.3",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.20.3",
    "ts-loader": "^4.4.2",
    "tslint": "^6.1.3",
    "tslint-loader": "^3.6.0",
    "typescript": "3.4.5",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "web-ext-types": "^3.2.1",
    "webpack": "4.16.1",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "@material-ui/icons": "^1.1.0",
    "@types/deep-equal": "^1.0.1",
    "@types/history": "^4.6.2",
    "@types/lodash": "^4.14.184",
    "@types/qrcode.react": "^0.8.0",
    "@types/scryptsy": "^2.0.0",
    "@types/secp256k1": "^4.0.3",
    "axios": "^0.27.2",
    "bignumber.js": "^7.2.1",
    "bip39": "^3.0.4",
    "bitcoinjs-lib": "4.0.5",
    "bitcoinjs-message": "^2.2.0",
    "chrome-call": "^3.0.0",
    "classnames": "^2.2.6",
    "deep-equal": "^2.0.5",
    "follow-redirects": "1.14.8",
    "metrixjs-wallet": "^0.2.20",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "mobx-react-router": "^4.0.4",
    "moment": "^2.29.4",
    "mweb3": "^1.0.0",
    "qrcode.react": "^0.8.0",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "secp256k1": "^4.0.3",
    "wif": "^2.0.6"
  }
